<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/bovenbanner_instellingen"
        android:layout_width="419dp"
        android:layout_height="63dp"
        android:layout_marginBottom="668dp"
        android:background="#8BC34A"
        android:text="@string/instellingen"
        android:textAlignment="center"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <TextView
        android:id="@+id/Algemene_instellingen"
        android:layout_width="459dp"
        android:layout_height="66dp"
        android:layout_marginTop="79dp"
        android:layout_marginBottom="586dp"
        android:gravity="center"
        android:text="@string/algemeneinstellingen"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dekast"
        android:layout_width="459dp"
        android:layout_height="66dp"
        android:layout_marginTop="364dp"
        android:layout_marginBottom="301dp"
        android:gravity="center"
        android:text="@string/de_kast"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>
