<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeKast">


    <Button
        android:id="@+id/btn_kastBackHome2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="36dp"
        android:background="@android:color/holo_green_dark"
        android:text="@string/previous"
        app:backgroundTint="@android:color/holo_green_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarKast"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="@android:color/holo_green_dark"
        android:tooltipText="de kast"
        app:popupTheme="@style/Theme.WhatsnextHomescreen.PopupOverlay"
        tools:ignore="MissingConstraints" />
    <ListView
        android:id="@+id/de_kast_listview"
        android:layout_width="match_parent"
        android:layout_height="590dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="51dp"
        tools:ignore="MissingConstraints"></ListView>
    <TextView
        android:id="@+id/txtKast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="De Kast"
        android:textSize="25dp"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="17dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>